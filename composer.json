{
    "name": "joedixon/nova-translation",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.1.0",
        "joedixon/laravel-translation": "^2.1.2"
    },
    "require-dev": {
        "laravel/pint": "1.22.0"
    },
    "scripts": {
        "lint": "./vendor/bin/pint",
        "lint:check": "./vendor/bin/pint --test -v"
    },
    "autoload": {
        "psr-4": {
            "Joedixon\\NovaTranslation\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Joedixon\\NovaTranslation\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
